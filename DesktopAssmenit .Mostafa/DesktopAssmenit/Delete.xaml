<Page x:Class="DesktopAssmenit.Delete"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:DesktopAssmenit"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Delete">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="19*"/>
            <RowDefinition Height="71*"/>
        </Grid.RowDefinitions>
        <Grid Background="White" Grid.RowSpan="2">
            <Button Style="{StaticResource buttonn}" Content="Delete" HorizontalAlignment="Left" Margin="520,10,0,0" VerticalAlignment="Top" Width="123" Height="42" Click="Button_Click"/>
            <TextBox x:Name="Phone" HorizontalAlignment="Left" Margin="210,18,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="210" Height="34"/>
            <DataGrid x:Name="MO" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="0,250,0,0"/>
        </Grid>
        <Grid Grid.RowSpan="2"/>
        <Label  Content="phone" HorizontalAlignment="Left" Margin="80,11,0,0" VerticalAlignment="Top" Width="85" FontSize="22"/>

    </Grid>
</Page>
